{
  "name": "loide-pwa",
  "description": "Progressive Web App IDE for Logic Programming",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/DeMaCS-UNICAL/LoIDE-PWA.git"
  },
  "bugs": {
    "url": "https://github.com/DeMaCS-UNICAL/LoIDE-PWA/issues"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "start:prod": "npm run build && node server.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --transformIgnorePatterns 'node_modules/(?!(@ionic/react|@ionic/react-router|@ionic/core|@stencil/core|ionicons)/)' --watchAll=false",
    "test:ci": "CI=true run-p types lint prettier:check 'test -- --coverage' --silent",
    "test:coverage": "CI=true npm test -- --env=jsdom --coverage --colors",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "types": "tsc --noEmit",
    "format:staged": "pretty-quick --staged",
    "prettier:check": "prettier --check ./src",
    "prettier:write": "prettier --write ./src",
    "prepare": "husky"
  },
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@ionic/core": "^8.7.5",
    "@ionic/react": "^8.7.5",
    "@ionic/react-router": "^8.7.5",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.3",
    "@reduxjs/toolkit": "^2.9.0",
    "@svgr/webpack": "^5.5.0",
    "ace-builds": "^1.23.4",
    "ajv": "^8.17.1",
    "babel-jest": "^27.4.2",
    "babel-loader": "^8.2.3",
    "babel-plugin-named-asset-import": "^0.3.8",
    "babel-preset-react-app": "^10.0.1",
    "bfj": "^7.0.2",
    "browserslist": "^4.18.1",
    "camelcase": "^6.2.1",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "compression": "^1.8.1",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "eslint": "^8.3.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-webpack-plugin": "^3.1.1",
    "express": "^4.21.2",
    "express-force-ssl": "^0.3.2",
    "express-rate-limit": "^7.5.1",
    "file-loader": "^6.2.0",
    "fs-extra": "^10.0.0",
