{
  "name": "loide-pwa",
  "description": "Progressive Web App IDE for Logic Programming",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/DeMaCS-UNICAL/LoIDE-PWA.git"
  },
  "bugs": {
    "url": "https://github.com/DeMaCS-UNICAL/LoIDE-PWA/issues"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:prod": "npm run build && node server.js",
    "build": "react-scripts build",
    "test": "react-scripts test --transformIgnorePatterns 'node_modules/(?!(@ionic/react|@ionic/react-router|@ionic/core|@stencil/core|ionicons)/)' --watchAll=false",
    "test:ci": "CI=true run-p types lint prettier:check 'test -- --coverage' --silent",
    "test:coverage": "CI=true npm test -- --env=jsdom --coverage --colors",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "types": "tsc --noEmit",
    "eject": "react-scripts eject",
    "format:staged": "pretty-quick --staged",
    "prettier:check": "prettier --check ./src",
    "prettier:write": "prettier --write ./src",
    "prepare": "husky"
  },
  "dependencies": {
    "@ionic/core": "^8.4.1",
    "@ionic/react": "^7.2.2",
    "@ionic/react-router": "^8.3.2",
    "@ionic/react-test-utils": "^0.4.0",
    "@reduxjs/toolkit": "^2.6.0",
    "ace-builds": "^1.23.4",
    "ajv": "^8.17.1",
    "compression": "^1.7.5",
    "express": "^4.21.2",
    "express-force-ssl": "^0.3.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^8.0.0",
    "ionicons": "^7.1.2",
    "json-pointer": "^0.6.2",
    "mousetrap": "^1.6.5",
    "react": "^18.2.0",
    "react-ace": "^10.1.0",
    "react-detect-offline": "^2.4.5",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.5",
    "react-redux": "^9.2.0",
    "react-router": "^5.3.3",
    "react-router-dom": "^5.3.4",
    "react-tabs": "^6.0.2",
    "redux": "^5.0.1",
    "socket.io-client": "^4.8.0",
    "web-vitals": "^3.4.0",
    "workbox-core": "^7.3.0",
    "workbox-expiration": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.3.0",
    "workbox-strategies": "^7.3.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^16.3.0",
    "@types/ace": "0.0.48",
    "@types/mousetrap": "^1.6.11",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.3.1",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@types/socket.io-client": "^1.4.36",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^9.1.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "pretty-quick": "^4.0.0",
    "react-scripts": "5.0.1",
    "sass": "^1.83.1"
  },
  "overrides": {
    "nth-check": "2.0.1",
    "postcss": "8.4.31"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
