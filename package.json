{
  "name": "loide-pwa",
  "description": "Progressive Web App IDE for Logic Programming",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/DeMaCS-UNICAL/LoIDE-PWA.git"
  },
  "bugs": {
    "url": "https://github.com/DeMaCS-UNICAL/LoIDE-PWA/issues"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:prod": "npm run build && node server.js",
    "build": "react-scripts build",
    "test": "react-scripts test --transformIgnorePatterns 'node_modules/(?!(@ionic/react|@ionic/react-router|@ionic/core|@stencil/core|ionicons)/)' --watchAll=false",
    "test:ci": "CI=true run-p types lint prettier:check 'test -- --coverage' --silent",
    "test:coverage": "CI=true npm test -- --env=jsdom --coverage --colors",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "types": "tsc --noEmit",
    "eject": "react-scripts eject",
    "format:staged": "pretty-quick --staged",
    "prettier:check": "prettier --check ./src",
    "prettier:write": "prettier --write ./src",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ionic/react": "^7.1.1",
    "@ionic/react-router": "^7.2.2",
    "@ionic/react-test-utils": "^0.4.0",
    "@reduxjs/toolkit": "^1.9.5",
    "ace-builds": "^1.23.4",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "express-force-ssl": "^0.3.2",
    "helmet": "^6.2.0",
    "ionicons": "^6.1.3",
    "mousetrap": "^1.6.5",
    "react": "^18.2.0",
    "react-ace": "^10.1.0",
    "react-detect-offline": "^2.4.5",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-redux": "^8.1.2",
    "react-router": "^5.3.3",
    "react-router-dom": "^5.3.4",
    "react-scripts": "5.0.1",
    "react-tabs": "^6.0.2",
    "sass": "^1.64.2",
    "socket.io-client": "^3.1.3",
    "web-vitals": "^2.1.4",
    "workbox-background-sync": "^6.5.4",
    "workbox-broadcast-update": "^6.5.4",
    "workbox-cacheable-response": "^6.5.4",
    "workbox-core": "^6.5.4",
    "workbox-expiration": "^6.5.4",
    "workbox-google-analytics": "^6.5.4",
    "workbox-navigation-preload": "^7.0.0",
    "workbox-precaching": "^6.5.4",
    "workbox-range-requests": "^6.5.4",
    "workbox-routing": "^6.5.4",
    "workbox-strategies": "^7.0.0",
    "workbox-streams": "^6.5.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/ace": "0.0.48",
    "@types/jest": "^27.5.2",
    "@types/mousetrap": "^1.6.11",
    "@types/node": "^20.4.6",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@types/socket.io-client": "^1.4.36",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest-watch-typeahead": "^2.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
